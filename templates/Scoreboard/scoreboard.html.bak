              <label class="modal-select-label" for="leftGoalPlayer{{ player.game_number }}">{{ player.game_number }}</label>
              {% endfor %}
          </div>
        </div>
      </div>
      <button type="submit" class="modal-goal-left__submit modal__submit">Готово</button>
    </div>
