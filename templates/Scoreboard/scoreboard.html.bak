        $(this).css({
          'background-color': '#b9c0ea',
        });
      });
      $('#rightDel').mouseup(function(){
        $(this).css({
          'background-color': '#fff',
        });
        createDelRight();
      });
      $('#minus').mousedown(function(){
        $(this).css({
          'background-color': 'rgb(20, 36, 138)',
        });
      });
      $('#minus').mouseup(function(){
        $(this).css({
          'background-color': '#3241A5',
